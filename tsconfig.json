{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "dom",
      "dom.iterable"
    ],
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
